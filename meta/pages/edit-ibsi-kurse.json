    }
  ],
  "events": {
    "load": [
      {
        "name": "dbHopeKurseTextbausteine.getIbsiKurseByKursNr",
        "parameters": [
          {
            "name": "KursNr",
            "value": "${parameters.KursNr}"
          }
        ],
        "then": [
          {
            "name": "ibsikurse",
            "type": "set",
            "value": "${result}"
          },
          {
            "name": "dbHopeKurseTextbausteine.getIbsiKurseArtenByKursArtNr",
            "parameters": [
              {
                "name": "KursArtNr",
                "value": "${this.ibsikurse.KursArtNr}"
              }
            ],
            "then": [
              {
                "name": "getByIbsiKurseArtensForKursArtNrResult",
                "type": "set",
                "value": "${result}"
              }
            ],
            "condition": "this.ibsikurse.KursArtNr != null",
            "type": "invoke"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "getIbsiKurseArtensForKursArtNrPageSize",
        "type": "set",
        "value": "10"
      },
      {
        "name": "dbHopeKurseTextbausteine.getIbsiKurseArtens",
        "parameters": [
          {
            "name": "$top",
            "value": "${getIbsiKurseArtensForKursArtNrPageSize}"
          },
          {
            "name": "$skip",
            "value": "0"
          },
          {
            "name": "$count",
            "value": "true"
          }
        ],
        "then": [
          {
            "name": "getIbsiKurseArtensForKursArtNrResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getIbsiKurseArtensForKursArtNrCount",
            "type": "set",
            "value": "result['@odata.count']"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Ibsi Kurse",
