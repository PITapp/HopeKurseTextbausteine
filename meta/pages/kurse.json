{
  "access": [
    "Authenticated"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "justify-content-start  col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 d-flex align-items-center",
              "components": [
                {
                  "name": "heading8",
                  "size": "H1",
                  "style": {
                    "color": "#5d717b",
                    "display": "inline-block"
                  },
                  "text": "Kurse",
                  "type": "heading"
                },
                {
                  "name": "heading9",
                  "size": "H3",
                  "style": {
                    "color": "#e5ecef",
                    "display": "inline-block",
                    "marginTop": "7px",
                    "paddingLeft": "10px",
                    "paddingRight": "10px"
                  },
                  "text": "|",
                  "type": "heading"
                },
                {
                  "attributes": [
                    {}
                  ],
                  "name": "heading2",
                  "size": "H3",
                  "style": {
                    "color": "#5d717b",
                    "display": "inline",
                    "fontSize": "21px",
                    "fontWeight": "normal"
                  },
                  "text": "${dsoKurse.Titel}",
                  "type": "heading"
                }
              ],
              "name": "col46",
              "style": {
                "height": "57px",
                "paddingBottom": "1px"
              },
              "type": "col"
            }
          ],
          "name": "row24",
          "style": {
            "height": "",
            "marginTop": "-7px",
            "paddingBottom": "0",
            "paddingLeft": "5px",
            "paddingRight": "3px",
            "paddingTop": "0",
            "width": ""
          },
          "type": "row"
        },
        {
          "items": [
            {
              "components": [
                {
                  "components": [
                    {
                      "class": "col-md-12",
                      "components": [
                        {
                          "components": [
                            {
                              "addText": "Add",
                              "allowAdd": false,
                              "allowDelete": true,
                              "allowFiltering": true,
                              "allowMultiSorting": false,
                              "allowPaging": true,
                              "allowSorting": true,
                              "applyFilterText": "Apply",
                              "clearFilterText": "Clear",
                              "columns": [
                                {
                                  "property": "Titel",
                                  "title": "Titel",
                                  "type": "string"
                                },
                                {
                                  "property": "Untertitel",
                                  "title": "Untertitel",
                                  "type": "string"
                                },
                                {
                                  "property": "Kurzzeichen",
                                  "title": "Kurzzeichen",
                                  "type": "string"
                                },
                                {
                                  "filterProperty": "IbsiKurseArten.Bezeichnung",
                                  "property": "KursArtNr",
                                  "sortProperty": "IbsiKurseArten.Bezeichnung",
                                  "template": "${data.IbsiKurseArten?.Bezeichnung}",
                                  "title": "Art",
                                  "type": "string"
                                },
                                {
                                  "property": "Beschreibung",
                                  "title": "Beschreibung",
                                  "type": "string"
                                },
                                {
                                  "property": "AnzahlThemen",
                                  "textAlign": "right",
                                  "title": "Anzahl Themen",
                                  "type": "integer"
                                },
                                {
                                  "formatString": "",
                                  "property": "Printkurs",
                                  "template": "${data.Printkurs == true ? 'Ja' : ''}",
                                  "textAlign": "center",
                                  "title": "Printkurs",
                                  "type": "string",
                                  "width": "100px"
                                },
                                {
                                  "property": "Onlinekurs",
                                  "template": "${data.Printkurs == true ? 'Ja' : ''}",
                                  "textAlign": "center",
                                  "title": "Onlinekurs",
                                  "type": "string",
                                  "width": "100px"
                                },
                                {
                                  "filterable": false,
                                  "sortable": false,
                                  "template": [
                                    {
                                      "buttonStyle": "light",
                                      "events": {
                                        "click": [
                                          {
                                            "parameters": [
                                              {
                                                "name": "AutorNr",
                                                "value": "${data.AutorNr}"
                                              }
                                            ],
                                            "path": "autoren-bearbeiten",
                                            "title": "Bearbeiten Autor",
                                            "type": "popup"
                                          }
                                        ]
                                      },
                                      "icon": "edit",
                                      "name": "editButton",
                                      "size": "sm",
                                      "text": "",
                                      "type": "button"
                                    }
                                  ],
                                  "textAlign": "center",
                                  "width": "75px"
                                }
                              ],
                              "confirmDelete": true,
                              "confirmDeleteCancelText": "Abbruch",
                              "confirmDeleteOkText": "Löschen",
                              "confirmDeleteText": "Bist du sicher, dass dieser Kurs gelöscht werden soll?",
                              "confirmDeleteTitle": "Löschen Kurs",
                              "containsText": "Contains",
                              "count": "${rstKurseCount}",
                              "data": "${rstKurse}",
                              "emptyText": "Keine Kurse vorhanden",
                              "endsWithText": "Ends with",
                              "equalsText": "Equals",
                              "events": {
                                "add": [],
                                "delete": [
                                  {
                                    "error": [
                                      {
                                        "detail": "",
                                        "severity": "error",
                                        "summary": "Kurs konnte nicht gelöscht werden!",
                                        "type": "notify"
                                      }
                                    ],
                                    "name": "dbHopeKurseTextbausteine.deleteIbsiKurse",
                                    "parameters": [
                                      {
                                        "name": "KursNr",
                                        "value": "${event.KursNr}"
                                      }
                                    ],
                                    "then": [
                                      {
                                        "detail": "",
                                        "severity": "success",
                                        "summary": "Kurs erfolgreich gelöscht",
                                        "type": "notify"
                                      }
                                    ],
                                    "type": "invoke"
                                  }
                                ],
                                "loadData": [
                                  {
                                    "error": [
                                      {
                                        "detail": "",
                                        "severity": "error",
                                        "summary": "Kurse konnten nicht geladen werden!",
                                        "type": "notify"
                                      }
                                    ],
                                    "name": "dbHopeKurseTextbausteine.getIbsiKurses",
                                    "parameters": [
                                      {
                                        "name": "$top",
                                        "value": "event.top"
                                      },
                                      {
                                        "name": "$skip",
                                        "value": "event.skip"
                                      },
                                      {
                                        "name": "$count",
                                        "value": "event.top != null && event.skip != null"
                                      },
                                      {
                                        "name": "$filter",
                                        "value": "${event.filter}"
                                      },
                                      {
                                        "name": "$orderby",
                                        "value": "${event.orderby}"
                                      },
                                      {
                                        "name": "$expand",
                                        "value": "IbsiKurseArten"
                                      }
                                    ],
                                    "then": [
                                      {
                                        "name": "rstKurse",
                                        "type": "set",
                                        "value": "${result.value}"
                                      },
                                      {
                                        "name": "rstKurseCount",
                                        "type": "set",
                                        "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                                      },
                                      {
                                        "code": "${this.gridKurse.onSelect(result.value[0])}",
                                        "condition": "result.value.length && !this.grid0.value",
                                        "type": "execute"
                                      }
                                    ],
                                    "type": "invoke"
                                  }
                                ],
                                "rowDoubleClick": [],
                                "rowSelect": [
                                  {
                                    "name": "dsoKurse",
                                    "type": "set",
                                    "value": "${event}"
                                  },
                                  {
                                    "name": "dbHopeKurseTextbausteine.getIbsiKurseThemens",
                                    "parameters": [
                                      {
                                        "name": "$filter",
                                        "value": "KursNr eq ${event.KursNr}"
                                      },
                                      {
                                        "name": "$expand",
                                        "value": "IbsiKurse"
                                      }
                                    ],
                                    "then": [
                                      {
                                        "name": "rstKurseThemen",
                                        "type": "set",
                                        "value": "${result.value}"
                                      }
                                    ],
                                    "type": "invoke"
                                  }
                                ]
                              },
                              "filterCaseSensitivity": "caseInsensitive",
                              "greaterThanOrEqualsText": "Greater than or equals",
                              "greaterThanText": "Greater than",
                              "lessThanOrEqualsText": "Less than or equals",
                              "lessThanText": "Less than",
                              "name": "gridKurse",
                              "notEqualsText": "Not equals",
                              "pageSize": 15,
                              "startsWithText": "Starts with",
                              "style": {
                                "border": "1px solid rgba(0, 0, 0, 0)",
                                "height": "700px",
                                "marginLeft": "-10px",
                                "marginRight": "-10px",
                                "marginTop": "-13px"
                              },
                              "title": "",
                              "type": "grid",
                              "value": "${dsoKurse}"
                            }
                          ],
                          "headerTemplate": [
                            {
                              "components": [
                                {
                                  "class": "col-md-12 align-items-start d-flex",
                                  "components": [
                                    {
                                      "events": {
                                        "click": [
                                          {
                                            "path": "kontakte-neu",
                                            "then": [
                                              {
                                                "code": "this.gridKontakte.load();",
                                                "condition": "${result} != null",
                                                "type": "execute"
                                              }
                                            ],
                                            "title": "Neuer Kontakt",
                                            "type": "popup"
                                          }
                                        ]
                                      },
                                      "icon": "add",
                                      "name": "button0",
                                      "size": "sm",
                                      "style": {
                                        "marginRight": "-9px"
                                      },
                                      "text": "Neuer Kurs",
                                      "type": "button"
                                    }
                                  ],
                                  "name": "col1",
                                  "type": "col"
                                }
                              ],
                              "name": "row0",
                              "type": "row"
                            }
                          ],
                          "name": "panel0",
                          "style": {
                            "border": "1px solid #e6ecef",
                            "height": "750px"
                          },
                          "text": " ${rstKurseCount} Kurse",
                          "type": "panel"
                        }
                      ],
                      "name": "col0",
                      "type": "col"
                    }
                  ],
                  "name": "row0",
                  "style": {
                    "height": "100%"
                  },
                  "type": "row"
                }
              ],
              "icon": "",
              "selected": true,
              "text": "Daten"
            },
            {
              "components": [],
              "icon": "",
              "selected": false,
              "text": "Themen"
            }
          ],
          "name": "tabs0",
          "style": {
            "float": "none"
          },
          "type": "tabs"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.gridKurse.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "Main",
  "name": "Kurse"
}