{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "IbsiKurse"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "name": "grid0",
                  "allowDelete": true,
                  "confirmDelete": true,
                  "allowAdd": true,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "count": "${getIbsiKursesCount}",
                  "data": "${getIbsiKursesResult}",
                  "events": {
                    "loadData": [
                      {
                        "name": "dbHopeKurseTextbausteine.getIbsiKurses",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "IbsiKurseArten"
                          }
                        ],
                        "then": [
                          {
                            "name": "getIbsiKursesResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getIbsiKursesCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          },
                          {
                            "code": "${this.grid0.onSelect(result.value[0])}",
                            "condition": "result.value.length && !this.grid0.value",
                            "type": "execute"
                          }
                        ],
                        "type": "invoke",
                        "error": [
                          {
                            "detail": "Unable to load IbsiKurses",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ]
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete IbsiKurse",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbHopeKurseTextbausteine.deleteIbsiKurse",
                        "parameters": [
                          {
                            "name": "KursNr",
                            "value": "${event.KursNr}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "IbsiKurse deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "add": [
                      {
                        "path": "add-ibsi-kurse",
                        "type": "popup"
                      }
                    ],
                    "rowDoubleClick": [
                      {
                        "parameters": [
                          {
                            "name": "KursNr",
                            "value": "${event.KursNr}"
                          }
                        ],
                        "path": "edit-ibsi-kurse",
                        "type": "popup"
                      }
                    ],
                    "rowSelect": [
                      {
                        "name": "master",
                        "type": "set",
                        "value": "${event}"
                      },
                      {
                        "name": "dbHopeKurseTextbausteine.getIbsiKurseThemens",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "KursNr eq ${event.KursNr}"
                          },
                          {
                            "name": "$expand",
                            "value": "IbsiKurse"
                          }
                        ],
                        "then": [
                          {
                            "name": "IbsiKurseThemens",
                            "type": "set",
                            "value": "${result.value}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "KursNr",
                      "title": "Kurs Nr",
                      "type": "integer"
                    },
                    {
                      "property": "KursArtNr",
                      "sortProperty": "IbsiKurseArten.Bezeichnung",
                      "filterProperty": "IbsiKurseArten.Bezeichnung",
                      "template": "${data.IbsiKurseArten?.Bezeichnung}",
                      "title": "Ibsi Kurse Arten",
                      "type": "string"
                    },
                    {
                      "property": "Titel",
                      "title": "Titel",
                      "type": "string"
                    },
                    {
                      "property": "Untertitel",
                      "title": "Untertitel",
                      "type": "string"
                    },
                    {
                      "property": "Kurzzeichen",
                      "title": "Kurzzeichen",
                      "type": "string"
                    },
                    {
                      "property": "Beschreibung",
                      "title": "Beschreibung",
                      "type": "string"
                    },
                    {
                      "property": "AnzahlThemen",
                      "title": "Anzahl Themen",
                      "type": "integer"
                    },
                    {
                      "property": "Printkurs",
                      "title": "Printkurs",
                      "type": "boolean"
                    },
                    {
                      "property": "Onlinekurs",
                      "title": "Onlinekurs",
                      "type": "boolean"
                    }
                  ],
                  "type": "grid",
                  "value": "${master}"
                },
                {
                  "name": "heading1",
                  "size": "H1",
                  "text": "IbsiKurseThemens",
                  "type": "heading"
                },
                {
                  "name": "grid1",
                  "data": "${this.IbsiKurseThemens}",
                  "columns": [
                    {
                      "property": "KursThemaNr",
                      "title": "Kurs Thema Nr",
                      "type": "integer"
                    },
                    {
                      "property": "KursNr",
                      "title": "Kurs Nr",
                      "type": "integer"
                    },
                    {
                      "property": "Nummer",
                      "title": "Nummer",
                      "type": "integer"
                    },
                    {
                      "property": "Titel",
                      "title": "Titel",
                      "type": "string"
                    },
                    {
                      "property": "Untertitel",
                      "title": "Untertitel",
                      "type": "string"
                    },
                    {
                      "property": "Kurzbeschreibung",
                      "title": "Kurzbeschreibung",
                      "type": "string"
                    },
                    {
                      "property": "Begleitmaterial",
                      "title": "Begleitmaterial",
                      "type": "string"
                    },
                    {
                      "property": "Druckfehler",
                      "title": "Druckfehler",
                      "type": "string"
                    },
                    {
                      "property": "InfoMemo",
                      "title": "Info Memo",
                      "type": "string"
                    }
                  ],
                  "type": "grid",
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "events": {
                    "add": [
                      {
                        "path": "add-ibsi-kurse-themen",
                        "type": "popup",
                        "parameters": [
                          {
                            "name": "KursNr",
                            "type": "integer",
                            "value": "${this.master.KursNr}"
                          }
                        ],
                        "then": []
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete IbsiKurseThemen",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbHopeKurseTextbausteine.deleteIbsiKurseThemen",
                        "parameters": [
                          {
                            "name": "KursThemaNr",
                            "value": "${event.KursThemaNr}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "IbsiKurseThemen deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "KursThemaNr",
                            "value": "${event.KursThemaNr}"
                          }
                        ],
                        "path": "edit-ibsi-kurse-themen",
                        "type": "popup",
                        "then": []
                      }
                    ]
                  },
                  "allowAdd": true,
                  "allowDelete": true,
                  "allowMultiSorting": false
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "type": "execute",
        "code": "this.grid0.load();"
      }
    ]
  },
  "name": "IbsiKurse",
  "access": "Authenticated",
  "layout": "Main"
}