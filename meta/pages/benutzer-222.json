{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Benutzer222"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "name": "grid0",
                  "allowDelete": true,
                  "confirmDelete": true,
                  "allowAdd": true,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "count": "${getBenutzersCount}",
                  "data": "${getBenutzersResult}",
                  "events": {
                    "loadData": [
                      {
                        "name": "dbHopeKurseTextbausteine.getBenutzers",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "Base"
                          }
                        ],
                        "then": [
                          {
                            "name": "getBenutzersResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getBenutzersCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke",
                        "error": [
                          {
                            "detail": "Unable to load Benutzers",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ]
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete Benutzer",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbHopeKurseTextbausteine.deleteBenutzer",
                        "parameters": [
                          {
                            "name": "BenutzerID",
                            "value": "${event.BenutzerID}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "Benutzer deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "add": [
                      {
                        "path": "add-benutzer-222",
                        "type": "popup"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "BenutzerID",
                            "value": "${event.BenutzerID}"
                          }
                        ],
                        "path": "edit-benutzer-222",
                        "type": "popup"
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "BenutzerID",
