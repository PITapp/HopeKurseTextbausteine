                      "sortProperty": "Base.Name1",
                      "filterProperty": "Base.Name1",
                      "template": "${data.Base?.Name1}",
                      "title": "Base",
                      "type": "string"
                    },
                    {
                      "property": "Benutzername",
                      "title": "Benutzername",
                      "type": "string"
                    },
                    {
                      "property": "AspNetUsers_Id",
                      "title": "Asp Net Users Id",
